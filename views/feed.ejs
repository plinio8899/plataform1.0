  <link rel="stylesheet" href="./css/light.css">
  <style>
    .bac{
        display: flex !important;
        align-items: center;
    }

    
    @keyframes fadeIn {
            from {
                opacity: 0;
                /* Elemento invisible al principio */
                transform: translateX(100px);
                /* Puede ajustarse según la dirección deseada */
            }

            to {
                opacity: 1;
                transform: translateX(0);
                /* Sin transformación al final */
            }
        }

        /* Aplica la animación al elemento */
        .fade {
            animation: fadeIn 0.3s ease-out forwards;
            /* Nombre de la animación, duración, y función de temporización */
        }
  </style>
    <title>Feed</title>
</head>
<body>
    <div class="container">
        <div class="bac">
            <a class="btn btn-dark" href="/dashboard?id=<%= id %>" style="margin-top: 1em;"><span class="material-symbols-outlined">
                arrow_back
                </span></a>
        </div>
        <br>
        <center>
            <h1 class="display-4">Estados</h1>
            <hr>
        </center>
        <br><br>
    </div>
</body>
